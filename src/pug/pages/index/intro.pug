-
  const names = [
    "Офисы",
    "Промышленная зона",
    "Ремонтные работы",
    "Жилая зона",
    "Парк отдыха",
    "Cтроительная площадка",
  ]

  const table = [
    {
      name: "Собственник",
      text: "ООО «ГИСГИС»"
    },
    {
      name: "Арендатор",
      text: "ООО «ГИСГИС»"
    },
    {
      name: "Размер",
      text: "90х90х90"
    },
    {
      name: "Классификация",
      text: "Категория «А»"
    },
    {
      name: "Срок аренды",
      text: "5 мес."
    },
  ]

  const choises = [
    "Офисы",
    "Промышленная зона",
    "Ремонтные работы",
    "Жилая зона",
    "Строительная площадка",
    "Парк отдыха"
  ]

+button("Связаться", null, null, null, "chat", "link")(class="intro__mobile-btn" href="tel:+7900000000")
section.intro.section
  img(src="assets/images/intro/cloud.png" alt="Облака" class="intro__cloud")
  img(src="assets/images/intro/cloud.png" alt="Облака" class="intro__cloud")
  .intro__content.site-content-container
    .intro__content-block
      .intro__heading-text-wrapper
        .intro__heading-text
          | Анализ данных в системе
        .intro__heading-subtext
          p
            | Настроим продукт и автоматизируем все необходимые процессы на основе технического задания
    .intro__content-block
      .intro__text-mobile-wrapper
        h1.intro__content-heading
          | Внедряем геоинформационные системы для решения ваших задач
        .intro__content-text
          p
            | Внедряем платформу Geometa, настраивая продукт и автоматизируя все необходимые процессы на основе технического задания.
    .intro__select
      .intro__select-heading.js-select-heading
        span.intro__select-heading-text Офисы
        +icon-mono("arrow-top-bottom")(width="16" heihgt="16" class="intro__select-heading-icon")
      ul.intro__select-choises.js-select-list
        for item, index in choises
          - let isActive = index === 0 ? "is-active" : null
          li.intro__select-choise.js-select-choice(data-index=index class=isActive)= item

    .intro-layer.is-locked
      for item, index in names
        - let isFirst = index === 0 ? "is-active" : null
        .intro-layer__name.js-intro-names(class=isFirst)= item
          if index !== 0
            +loader("1rem", "1rem")

      .js-layer-container
        picture
          source(type="image/webp" srcset="assets/images/intro/layers/layer0_web.webp" alt="Layer")
          img(src="assets/images/intro/layers/layer0_0.png" alt="Layer" class="intro-layer__image js-intro-layer is-active")
        picture
          source(type="image/webp" srcset="assets/images/intro/colored-full.webp" alt="Layer")
          img(src="assets/images/intro/colored-full.png" alt="Layer" class="intro-layer__image _mobile")
      .intro-layer__tooltips
        .intro-tooltip.js-tooltip.is-choosen(style={"top": "29%", "left": "37%"} data-tooltip-index="0")
          button.intro-tooltip__btn
            span
            span
          .intro-tooltip__content
            .intro-tooltip__inner
